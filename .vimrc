"vunlde.vimで管理してるpluginを読み込む
source ~/.vimrc.conf/vimrc.bundle

"基本設定
source ~/.vimrc.conf/vimrc.basic
"StatusLine設定
source ~/.vimrc.conf/vimrc.statusline
"インデント設定
source ~/.vimrc.conf/vimrc.indent
"表示関連
source ~/.vimrc.conf/vimrc.apperance
"補完関連
source ~/.vimrc.conf/vimrc.completion
"Tags関連
source ~/.vimrc.conf/vimrc.tags
"検索関連
source ~/.vimrc.conf/vimrc.search
"移動関連
source ~/.vimrc.conf/vimrc.moving
"Color関連
source ~/.vimrc.conf/vimrc.colors
"編集関連
source ~/.vimrc.conf/vimrc.editing
"エンコーディング関連
source ~/.vimrc.conf/vimrc.encoding
"その他
source ~/.vimrc.conf/vimrc.misc
"プラグインに依存するアレ
source ~/.vimrc.conf/vimrc.plugins_setting

"Vimでgitのログをきれいに表示する - derisの日記
"  http://deris.hatenablog.jp/entry/2013/05/10/003430
" source ~/.vimrc.conf/vimrc.gitlogviewer

" added by hideyuki
source ~/.vimrc.conf/vimrc.hideyuki

